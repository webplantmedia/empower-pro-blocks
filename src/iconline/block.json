		"icon": {
			"type": "string",
			"default": "home"
		},
		"iconSize": {
			"type": "number",
			"default": ""
		},
		"marginBottom": {
			"type": "number",
			"default": ""
		},
		"topOffset": {
			"type": "number",
			"default": ""
		},
		"iconSpacing": {
			"type": "number",
			"default": "" 
		},
		"text": {
			"type": "string",
